{
    "complex_fields": [
        "accural_bal_sheet",
        "accural_cash_flow",
        "financial_health"
    ],
    "direct_fields": {
        "account_receivable": {
            "enabled": true,
            "freq": "quarterly",
            "raw_field": "BS_ACCT_NOTE_RCV"
        },
        "book_value": {
            "enabled": true,
            "freq": "quarterly",
            "raw_field": "CURR_BOOK_VAL"
        },
        "capital_expenditures": {
            "enabled": true,
            "freq": "quarterly",
            "raw_field": "ARD_CAPITAL_EXPENDITURES"
        },
        "cash_cash_equivalents": {
            "enabled": true,
            "freq": "quarterly",
            "raw_field": "ARD_CASH_CASH_EQUIV_END_OF_PER"
        },
        "cash_from_financing": {
            "enabled": true,
            "freq": "quarterly",
            "raw_field": "ARD_TOT_CASHFLOWS_FROM_FINANCING"
        },
        "cash_from_investing": {
            "enabled": true,
            "freq": "quarterly",
            "raw_field": "ARD_TOT_CASHFLOWS_FROM_INVESTING"
        },
        "cash_from_operating": {
            "enabled": true,
            "freq": "quarterly",
            "raw_field": "CF_CASH_FROM_OPER"
        },
        "depreciation": {
            "enabled": true,
            "freq": "quarterly",
            "raw_field": "CF_DEPR_AMORT"
        },
        "diluted_normalised_eps": {
            "enabled": true,
            "freq": "quarterly",
            "raw_field": "ARD_DILUTED_EPS"
        },
        "dividends": {
            "enabled": true,
            "freq": "quarterly",
            "raw_field": "CF_DVD_PAID"
        },
        "gross_profit": {
            "enabled": true,
            "freq": "quarterly",
            "raw_field": "IS_ADJUSTED_GROSS_PROFIT"
        },
        "income_before_tax": {
            "enabled": true,
            "freq": "quarterly",
            "raw_field": "ARD_INCOME_BEFORE_INCOME_TAXES"
        },
        "income_tax_expense": {
            "enabled": true,
            "freq": "quarterly",
            "raw_field": "IS_INC_TAX_EXP"
        },
        "last_price": {
            "enabled": true,
            "freq": "daily",
            "raw_field": "PX_LAST"
        },
        "net_change_in_cash": {
            "enabled": true,
            "freq": "quarterly",
            "raw_field": "ARD_NET_CHANGE_IN_CASH"
        },
        "net_income": {
            "enabled": true,
            "freq": "quarterly",
            "raw_field": "ARD_NET_INC"
        },
        "net_income_before_xo_items": {
            "enabled": true,
            "freq": "quarterly",
            "raw_field": "ARD_INCOME_BEFORE_XO_ITEMS"
        },
        "opening_price": {
            "enabled": true,
            "freq": "daily",
            "raw_field": "PX_OPEN"
        },
        "operating_income": {
            "enabled": true,
            "freq": "quarterly",
            "raw_field": "IS_OPER_INC"
        },
        "short_term_investments": {
            "enabled": true,
            "freq": "quarterly",
            "raw_field": "BS_MKT_SEC_OTHER_ST_INVEST"
        },
        "short_term_liabilities": {
            "enabled": true,
            "freq": "quarterly",
            "raw_field": "BS_ST_BORROW"
        },
        "total_assets": {
            "enabled": true,
            "freq": "quarterly",
            "raw_field": "ARD_TOT_ASSETS"
        },
        "total_current_assets": {
            "enabled": true,
            "freq": "quarterly",
            "raw_field": "ARD_TOTAL_CUR_ASSETS"
        },
        "total_current_liabilities": {
            "enabled": true,
            "freq": "quarterly",
            "raw_field": "ARD_TOTAL_CURRENT_LIABILITIES"
        },
        "total_debt": {
            "enabled": true,
            "freq": "quarterly",
            "raw_field": "SHORT_AND_LONG_TERM_DEBT"
        },
        "total_equity": {
            "enabled": true,
            "freq": "quarterly",
            "raw_field": "ARD_TOTAL_SHAREHOLDERS_EQUITY"
        },
        "total_inventories": {
            "enabled": true,
            "freq": "quarterly",
            "raw_field": "ARD_INVENTORY"
        },
        "total_liabilities": {
            "enabled": true,
            "freq": "quarterly",
            "raw_field": "ARD_TOT_LIABILITIES"
        },
        "total_long_term_debt": {
            "enabled": true,
            "freq": "quarterly",
            "raw_field": "ARD_LT_DEBT"
        },
        "total_revenue": {
            "enabled": true,
            "freq": "quarterly",
            "raw_field": "ARD_TOTAL_REVENUES"
        },
        "total_shares": {
            "enabled": true,
            "freq": "quarterly",
            "raw_field": "EQY_SH_OUT"
        },
        "volume": {
            "enabled": true,
            "freq": "daily",
            "raw_field": "PX_VOLUME"
        }
    },
    "indirect_fields": {
        "accural_cash_flow": "%net_income_before_xo_items% - %cash_from_operating%",
        "book_value_minus_debt": "%book_value% - %total_debt%",
        "cash_to_assets": "%cash_cash_equivalents% / %total_assets%",
        "debt_to_assets": "%total_debt% / %total_assets%",
        "debt_to_equity": "%total_debt% / %total_equity%",
        "dividend_payout_ratio": "%dividends% / %net_income%",
        "income_after_tax": "%income_before_tax% - %income_tax_expense%",
        "liabilities_to_income": "%total_liabilities% / %net_income%",
        "quick_ratio": "(%total_current_assets% - %total_inventories%)/%total_current_liabilities%",
        "receivalbes_to_sales": "%account_receivable% / %total_revenue%",
        "return_on_equity": "%net_income% / %total_equity%",
        "sales_per_shares": "%total_revenue% / %total_shares%",
        "snapshot_accurual": "%total_current_assets% - %cash_cash_equivalents% - %total_current_liabilities% + %total_debt%",
        "working_capital": "%total_current_assets% - %total_current_liabilities%"
    }
}