    "indirect_fields": {
        "accrual_cash_flow": "%net_income_before_xo_items% - %cash_from_operating%",
        "book_value_minus_debt": "%book_value% - %total_debt%",
        "book_value_yield": "1 / %price_to_book_ratio% * 100",
        "cash_flow_yield": "%cash_flow_per_share% / %last_price% * 100",
        "cash_flow_to_total_ssets": "%cash_from_operating% / %total_assets% * 100",
        "cash_to_assets": "%cash_cash_equivalents% / %total_assets%",
        "cfroic": "%cash_from_operating% / %total_invested_capital% * 100",
        "debt_to_assets": "%total_debt% / %total_assets%",
        "debt_to_equity": "%total_debt% / %total_equity%",
        "earnings_cv_fy1": "%best_eps_stddev_fy1% / %best_eps_mean_fy1% * 100",
        "earnings_cv_fy2": "%best_eps_stddev_fy2% / %best_eps_mean_fy2% * 100",
        "earnings_cv_mean": " (%earnings_cv_fy1% + %earnings_cv_fy2%)/2",
        "earnings_growth_fy1_to_fy2": "(%best_eps_fy1% - %best_eps_fy2%) / %best_eps_fy1% * 100",
        "earnings_yield_1yr_fwd": "1 / %best_pe_ratio% * 100",
        "earnings_yield_1yr_fwd_3_yr_hist": "%lt_price_earnings_ratio%/ %best_pe_ratio% * 100",
        "ebitda_to_ev": "1 / %current_ev_to_t12m_ebitda% * 100",
        "free_cf_to_ev": "1 / %ev_to_trailing_12m_free_cf% * 100",
        "income_after_tax": "%income_before_tax% - %income_tax_expense%",
        "liabilities_to_income": "%total_liabilities% / %net_income%",
        "net_earning_rev_fy1": "(%best_eps_numup_4w_fy1% - %best_eps_numdn_4w_fy1%) / (%best_eps_numup_4w_fy1% + %best_eps_numdn_4w_fy1%)*100 ",
        "net_earning_rev_fy2": "(%best_eps_numup_4w_fy2% - %best_eps_numdn_4w_fy2%) / (%best_eps_numup_4w_fy2% + %best_eps_numdn_4w_fy2%)*100 ",
        
        "quick_ratio": "(%total_current_assets% - %total_inventories%)/%total_current_liabilities%",
        "receivalbes_to_sales": "%account_receivable% / %total_revenue%",
        "return_on_equity": "%net_income% / %total_equity%",
        "sales_per_shares": "%total_revenue% / %total_shares%",
        "sales_to_ev": "1 / %ev_to_trailing_12m_sales% * 100",
        "sales_yield": "1 / %price_to_sales_ratio% * 100",
        "sales_yield_mean": "(%sales_yield_fy1% + %sales_yield_fy1%) / 2",
        "short_int_to_sh_out": " %short_interest% / %total_shares% * 100",
        "snapshot_accrual": "%total_current_assets% - %cash_cash_equivalents% - %total_current_liabilities% + %total_debt%",
        "working_capital": "%total_current_assets% - %total_current_liabilities%"
    }
}